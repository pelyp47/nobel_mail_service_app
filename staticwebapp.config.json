{
    "routes": [
      {
        "route": "/*.html",
        "redirect": "/index.html"
      },
      {
        "route": "/*.js",
        "redirect": "/app/assets/index.js"
      },
      {
        "route": "/*.css",
        "redirect": "/app/assets/index.css"
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": [
        "/assets/*",
        "/*.css",
        "/*.js",
        "/*.png",
        "/*.jpg",
        "/*.jpeg",
        "/*.gif",
        "/*.svg",
        "/*.webp",
        "/*.ico"
    ]
    }
}